<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sachin.dharm_fab">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ImageListActivity" />
        <activity android:name=".CategoryList" />
        <activity android:name=".ContactUs" />
        <activity android:name=".Home" />
        <activity android:name=".Enquiry" />
        <activity android:name=".CompanyDetail" />
        <activity android:name=".UploadItem" />
        <activity android:name=".SingleCategory" />
        <activity android:name=".Login"
            android:screenOrientation="portrait"/>
        <activity android:name=".EnquiryListActivity" />
        <activity android:name=".AdminPanel">
        </activity>
    </application>

</manifest>